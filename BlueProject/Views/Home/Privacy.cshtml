@{
    var dt = ViewData["dt"] as System.Data.DataTable;
}
<div class="table-responsive">
    <table id="zero_config" class="table table-striped table-bordered no-warp">
        <thead>
        <tr>
            <th>
                Status
            </th>
            <th>
                Title
            </th>
            <th>
                ID
            </th>
        </tr>
        </thead>
        <tbody>
        @foreach (System.Data.DataRow row in dt.Rows)
        {
            <tr>
                <td>@row["status"]</td>
                <td>@row["title"]</td>
                <td>@row["ticket_id"]</td>
            </tr>
        }
        </tbody>
    </table>      
</div>
<div>
    <form id="form1", method="post" action="/home/privacychange">
        <input name="ticket_id" type="text" value="1"/>
        <input name="title" type="text" value="Changed"/>
        <button type="submit" class="btn btn-sm btn-primary">Submit</button>
    </form>
</div>